<?xml version="1.0" encoding="UTF-8"?> 
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"> 
<mapper namespace="model"> 

<insert id="insert" parameterType="Model"  keyProperty = "id" useGeneratedKeys = "true">
insert into xinhuawang (title,origin,content,time,url) values(#{title},#{origin},#{content},#{time},#{url})
</insert>
</mapper>